{
  "$schema": "./node_modules/midi-mixer-plugin/plugin.schema.json",
  "id": "ca.jaggernaut.midi-mixer.voicemeeter",
  "name": "Voicemeeter",
  "type": "node",
  "version": "1.4.1",
  "author": "Jaggernaut",
  "main": "lib/index.js",
  "icon": "voicemeeter.ico",
  "settings": {
    "maxdb": {
      "label": "Max db. (Reload after changing this)",
      "type": "slider",
      "fallback": 12,
      "min": -60,
      "max": 12
    },
    "mindb": {
      "label": "Min db. (Reload after changing this)",
      "type": "slider",
      "fallback": -60,
      "min": -60,
      "max": 12
    },
    "BusToggles": {
      "label": "Bus Toggles (Reload after changing this)",
      "type": "text",
      "fallback": "strip0:A1,!B2;strip1:A1"
    },
    "vmstatus": {
      "label": "Voicemeeter Status",
      "type": "status",
      "fallback": "Not connected"
    },
    "runbutton": {
      "label": "run",
      "type": "button"
    }
  }
}